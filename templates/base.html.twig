<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{% block stylesheets %}
			<link rel="stylesheet" href="{{ asset('css/styles.css') }}">
			<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
		{% endblock %}
		{% block javascripts %}
			<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>

			<script>
				document.addEventListener('DOMContentLoaded', e => {
$('#input-prefetch').autocomplete()
}, false);
			</script>
		{% endblock %}
	</head>
	<body></body>

	{# NAVBAR #}
	<nav class="navbarcolor navbar">
		<div class="container-fluid">
			<a class="navbar-brand" href="#">
				<img src="{{asset('logo.png')}}" height="200px" alt="Logo" class="d-inline-block align-text-top">
				{% for caliber in caliber_list %}
				<div style=" padding: 20px">
				<a href="{{ path('app_caliber',{ 'caliber_id': caliber.id })}}">
					<button class="btn btn-outline-success" type="button">{{ caliber.name }}</button>
				</a>
				</div>
				{% endfor %}
			</a>
		</div>
	</nav>


	<div class="container"> {% block body %}{% endblock %}

		</html>
	</html>

